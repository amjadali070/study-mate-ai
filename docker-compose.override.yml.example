# Copy this file to docker-compose.override.yml and fill in your actual API keys
version: '3.8'

services:
  server:
    environment:
      # OpenAI API Key - Get from https://platform.openai.com/api-keys
      OPENAI_API_KEY: "sk-your-actual-openai-api-key-here"
      
      # Pinecone Configuration - Get from https://www.pinecone.io/
      PINECONE_API_KEY: "your-actual-pinecone-api-key-here"
      PINECONE_ENVIRONMENT: "your-pinecone-environment"  # e.g., "us-west1-gcp"
      PINECONE_INDEX_NAME: "rag-embeddings"
      
      # JWT Secret - Generate a secure random string
      JWT_SECRET: "your-super-secure-random-jwt-secret-key"
      
      # Development overrides
      NODE_ENV: "development"
